{
    "id": "com.layer.sync",
    "name": "LayerSync",
    "version": "1.0.0",
    "main": "index.html",
    "manifestVersion": 5,
    "host": {
        "app": "PS",
        "minVersion": "24.4.0"
    },
    "icons": [
        {
            "width": 23,
            "height": 23,
            "path": "icons/dark.png",
            "scale": [1, 2],
            "theme": ["darkest", "dark", "medium"]
        },
        {
            "width": 23,
            "height": 23,
            "path": "icons/light.png",
            "scale": [1, 2],
            "theme": ["lightest", "light"]
        }
    ],
    "entrypoints": [
        {
            "type": "panel",
            "id": "LayerSync",
            "minimumSize": {"width": 300, "height": 400},
            "maximumSize": {"width": 1000, "height": 1000},
            "preferredDockedSize": {"width": 300, "height": 400},
            "label": {
                "default": "图层同步显示"
            },
            "icons": [
                {
                    "width": 23,
                    "height": 23,
                    "path": "icons/dark.png",
                    "scale": [1, 2],
                    "theme": ["darkest", "dark", "medium"]
                },
                {
                    "width": 23,
                    "height": 23,
                    "path": "icons/light.png",
                    "scale": [1, 2],
                    "theme": ["lightest", "light"]
                }
            ]
        }
    ],
    "equiredPermissions": {
        "network": {
            "domains": [
                "https://spectrum.adobe.com"
            ]
        }
    }
}