{
    "id": "com.layer.sync",
    "name": "LayerSync",
    "version": "1.0.0",
    "main": "index.html",
    "manifestVersion": 5,
    "host": {
        "app": "PS",
        "minVersion": "24.4.0"
    },
    "entrypoints": [
        {
            "type": "panel",
            "id": "LayerSync",
            "minimumSize": {"width": 300, "height": 200},
            "maximumSize": {"width": 1000, "height": 1000},
            "preferredDockedSize": {"width": 300, "height": 200},
            "label": {"default": "图层同步"},
            "icons": [
                {
                    "width": 23,
                    "height": 23,
                    "path": "icons/dark.png",
                    "scale": [1, 2],
                    "theme": ["darkest", "dark", "medium", "lightest", "light", "all"]
                }
            ]
        }
    ]
}